{
  "name": "mern-starter",
  "version": "1.0.0",
  "description": "A personalized boilerplate to use with MERN applications. Includes dependencies for multiple features, such as Mongoose, Passport.js, axios, etc.",
  "main": "index.js",
  "engines": {
    "node": "8.1.1",
    "npm": "5.0.3"
  },
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JCayabyab/mern-starter.git"
  },
  "author": "Jofred Cayabyab",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JCayabyab/mern-starter/issues"
  },
  "homepage": "https://github.com/JCayabyab/mern-starter#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "concurrently": "^3.6.1",
    "cookie-session": "^2.0.0-beta.3",
    "express": "^4.16.3",
    "mongoose": "^5.2.9",
    "nodemon": "^1.18.3",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "redux-thunk": "^2.3.0"
  }
}
